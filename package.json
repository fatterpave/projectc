{
  "name": "react-mobx-material-boilerplate",
  "version": "0.0.1",
  "description": "Easy and Understandable React, MobX, Material-Ui with Css-Modules boilerplate",
  "repository": {
    "type": "git",
    "url": "git@github.com:Prattjames/react-mobx-material-boilerplate.git"
  },
  "scripts": {
    "start": "webpack-dev-server --config webpack-dev-server.config.js --progress --inline --colors",
